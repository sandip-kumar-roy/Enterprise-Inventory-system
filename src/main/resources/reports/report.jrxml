	<queryString>
		<![CDATA[SELECT
    u.user_id,
    u.username,
    r.role_name,
    p.*
FROM users u
INNER JOIN profile p  ON u.profile_id = p.profile_id
INNER JOIN role    r  ON u.fk_role_id = r.role_id]]>
